<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainMenu">

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Snack"
        android:elegantTextHeight="true"
        android:textSize="20sp"
        android:id="@+id/buttonSnack"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp" />

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Retaurants"
        android:textSize="20sp"
        android:textAlignment="center"
        android:id="@+id/buttonResto"
        android:layout_below="@+id/buttonSnack"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="43dp" />

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:textSize="20sp"
        android:text="Favoris"
        android:textAlignment="center"
        android:id="@+id/buttonFavoris"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Carte"
        android:textSize="20sp"
        android:textAlignment="center"
        android:id="@+id/buttonCarte"
        android:layout_below="@+id/buttonFavoris"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="51dp" />

</RelativeLayout>
